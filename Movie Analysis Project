            "t = -10.225938786018832 p = 2.9005542772112095e-24\n"
          ]
        }
      ],
      "source": [
        "from scipy import stats\n",
        "\n",
        "action = movies_exp[movies_exp[\"genre_list\"]==\"Action\"][\"avg_rating\"].dropna()\n",
        "romance = movies_exp[movies_exp[\"genre_list\"]==\"Romance\"][\"avg_rating\"].dropna()\n",
        "\n",
        "t_stat, p_val = stats.ttest_ind(action, romance, equal_var=False)\n",
        "print(\"t =\", t_stat, \"p =\", p_val)"
      ]
    }
