      "source": [
        "import pandas as pd\n",
        "from pathlib import Path\n",
        "\n",
        "# 1) Set file paths as STRINGS (do NOT call read_csv here)\n",
        "movies_path  = r\"\\\\ad.wayne.edu\\Users\\ex4360\\Documents\\Movie project\\movies.csv\"\n",
        "ratings_path = r\"\\\\ad.wayne.edu\\Users\\ex4360\\Documents\\Movie project\\ratings.csv\"\n",
        "tags_path    = r\"\\\\ad.wayne.edu\\Users\\ex4360\\Documents\\Movie project\\tags.csv\"  # optional\n",
        "\n",
        "# 2) Read the files (now call read_csv)\n",
        "movies  = pd.read_csv(movies_path)\n",
        "# Start with a manageable slice of ratings to avoid RAM issues\n",
        "ratings = pd.read_csv(ratings_path, usecols=[\"movieId\",\"rating\"], nrows=1_000_000)  # bump later if OK\n",
        "\n",
        "# 3) Make per-movie stats from ratings (avg + count)\n",
        "ratings_summary = (\n",
        "    ratings\n",
        "      .groupby(\"movieId\")[\"rating\"]\n",
        "      .agg(avg_rating=\"mean\", rating_count=\"count\")\n",
        "      .reset_index()\n",
        ")\n",
        "\n",
        "# 4) Merge summary back to movies\n",
        "movies_with_stats = movies.merge(ratings_summary, on=\"movieId\", how=\"left\")\n",
        "\n",
        "print(movies_with_stats.head())"
      ]
    },
